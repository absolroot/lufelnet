---
layout: null
sitemap:
  exclude: true
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:xhtml="http://www.w3.org/1999/xhtml">
  {% for page in site.pages %}
    {% unless page.url contains '.xml' or page.url contains 'assets' %}
      <url>
        <loc>{{ site.url }}{{ page.url | remove: "index.html" }}</loc>
        {% if page.sitemap.lastmod %}
          <lastmod>{{ page.sitemap.lastmod | date: "%Y-%m-%d" }}</lastmod>
        {% elsif page.date %}
          <lastmod>{{ page.date | date: "%Y-%m-%d" }}</lastmod>
        {% else %}
          <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        {% endif %}
        {% if page.sitemap.changefreq %}
          <changefreq>{{ page.sitemap.changefreq }}</changefreq>
        {% else %}
          <changefreq>daily</changefreq>
        {% endif %}
        {% if page.sitemap.priority %}
          <priority>{{ page.sitemap.priority }}</priority>
        {% else %}
          <priority>1.0</priority>
        {% endif %}
        {% if page.alternate_urls %}
          <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}{{ page.url | remove: 'index.html' }}" />
          {% for lang in page.alternate_urls %}
            <xhtml:link rel="alternate" hreflang="{{ lang[0] }}" href="{{ site.url }}{{ lang[1] }}" />
          {% endfor %}
          <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}{{ page.url | remove: 'index.html' }}" />
        {% endif %}
      </url>
    {% endunless %}
  {% endfor %}
  
  <!-- App pages with hreflang alternates (KR/EN/JP) -->
  <!-- Material Calculator -->
  <url>
    <loc>{{ site.url }}/material-calc/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/material-calc/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/material-calc/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/material-calc/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/material-calc/" />
  </url>

  <!-- Defense Calculator -->
  <url>
    <loc>{{ site.url }}/defense-calc/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/defense-calc/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/defense-calc/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/defense-calc/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/defense-calc/" />
  </url>

  <!-- Persona -->
  <url>
    <loc>{{ site.url }}/persona/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/persona/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/persona/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/persona/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/persona/" />
  </url>

  <!-- Synergy -->
  <url>
    <loc>{{ site.url }}/synergy/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/synergy/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/synergy/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/synergy/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/synergy/" />
  </url>

  <!-- Synergy Character Pages -->
  {% assign synergy_characters = "메로페,루페르,모토하,토모코,키요시,아야카,마사키,리코,마유미,마나카,카타야마,쇼키,슌,야오링,카요,미유,몽타뉴,미오,미나미,YUI,유우미,키라,루우나,이치고,무스비" | split: "," %}
  {% assign synergy_codenames = "merope,cattle,closer,kotomo,kii,chord,cherish,wind,turbo,ange,blitz,luce,soy,rin,okyann,puppet,mont,matoi,marian,bui,phoebe,doctor,howler,berry,yui,무스비" | split: "," %}
  {% assign languages = "kr,en,jp" | split: "," %}
  
  {% for character in synergy_characters %}
    {% assign char_index = forloop.index0 %}
    {% assign codename = synergy_codenames[char_index] %}
    
    <!-- Synergy Character: {{ character }} ({{ codename }}) -->
    <url>
      <loc>{{ site.url }}/synergy/?character={{ codename }}</loc>
      <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
      <changefreq>monthly</changefreq>
      <priority>0.7</priority>
      {% for lang in languages %}
        <xhtml:link rel="alternate" hreflang="{{ lang }}" href="{{ site.url }}/synergy/?character={{ codename }}&amp;lang={{ lang }}" />
      {% endfor %}
      <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/synergy/?character={{ codename }}" />
    </url>
    
    <!-- Synergy Character with explicit language parameters -->
    {% for lang in languages %}
      <url>
        <loc>{{ site.url }}/synergy/?character={{ codename }}&amp;lang={{ lang }}</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.6</priority>
        {% for alt_lang in languages %}
          <xhtml:link rel="alternate" hreflang="{{ alt_lang }}" href="{{ site.url }}/synergy/?character={{ codename }}&amp;lang={{ alt_lang }}" />
        {% endfor %}
        <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/synergy/?character={{ codename }}" />
      </url>
    {% endfor %}
  {% endfor %}

  <!-- Pull Tracker: Global Stats -->
  <url>
    <loc>{{ site.url }}/pull-tracker/global-stats/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/kr/pull-tracker/global-stats/" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/en/pull-tracker/global-stats/" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/jp/pull-tracker/global-stats/" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/pull-tracker/global-stats/" />
  </url>

  <!-- Revelations -->
  <url>
    <loc>{{ site.url }}/revelations/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/revelations/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/revelations/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/revelations/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/revelations/" />
  </url>

  <!-- Wonder Weapons -->
  <url>
    <loc>{{ site.url }}/wonder-weapon/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/wonder-weapon/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/wonder-weapon/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/wonder-weapon/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/wonder-weapon/" />
  </url>

  <!-- Release Schedule -->
  <url>
    <loc>{{ site.url }}/schedule/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/schedule/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/schedule/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/schedule/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/schedule/" />
  </url>

  <!-- Pull Planner -->
  <url>
    <loc>{{ site.url }}/pull-calc/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/kr/pull-calc/" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/en/pull-calc/" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/jp/pull-calc/" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/pull-calc/" />
  </url>

  <!-- Tactics Library -->
  <url>
    <loc>{{ site.url }}/tactic/tactics.html</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/tactic/tactics.html?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/tactic/tactics.html?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/tactic/tactics.html?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/tactic/tactics.html" />
  </url>

  <!-- Tactic Maker -->
  <url>
    <loc>{{ site.url }}/tactic-maker/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/tactic-maker/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/tactic-maker/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/tactic-maker/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/tactic-maker/" />
  </url>

  <!-- Astrolabe -->
  <url>
    <loc>{{ site.url }}/astrolabe/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>daily</changefreq>
    <priority>0.9</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/astrolabe/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/astrolabe/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/astrolabe/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="zh-CN" href="{{ site.url }}/astrolabe/?lang=cn" />
    <xhtml:link rel="alternate" hreflang="zh-TW" href="{{ site.url }}/astrolabe/?lang=tw" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/astrolabe/" />
  </url>

  <!-- Maps -->
  <url>
    <loc>{{ site.url }}/maps/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/maps/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/maps/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/maps/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/maps/" />
  </url>

  <!-- Tier List and Tier Maker specific URLs -->
  <!-- Tier List (default) -->
  <url>
    <loc>{{ site.url }}/tier/position-tier/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.9</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/tier/position-tier/?lang=kr" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/tier/position-tier/?lang=en" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/tier/position-tier/?lang=jp" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/tier/position-tier/" />
  </url>
  
  <!-- Tier Maker -->
  <url>
    <loc>{{ site.url }}/tier/position-tier/?list=false</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
    <xhtml:link rel="alternate" hreflang="ko" href="{{ site.url }}/tier/position-tier/?lang=kr&amp;list=false" />
    <xhtml:link rel="alternate" hreflang="en" href="{{ site.url }}/tier/position-tier/?lang=en&amp;list=false" />
    <xhtml:link rel="alternate" hreflang="ja" href="{{ site.url }}/tier/position-tier/?lang=jp&amp;list=false" />
    <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/tier/position-tier/?list=false" />
  </url>
  
  <!-- Character Pages -->
  {% comment %}
  Character list from data/character_info.js
  Main Party: 준페이, J&C, 슌·프론티어. 후카, 쇼키·암야, YUI·스텔라,미유·여름, 카타야마 ,렌, 루우나, 루페르, 레오, 류지, 리코·매화, 마사키, 마유미, 마코토, 미나미, 미나미·여름, 모르가나, 모토하, 모토하·여름, 몽타뉴, 몽타뉴·백조, 미오, 사나다, 슌, 세이지, 안, 아야카, 아케치, 야오링, 야오링·사자무, 원더, 유스케, YUI, 유카리, 유키미, 유키 마코토, 쇼키, 이치고, 카스미, 키라, 키요시, 토모코, 토모코·여름, 토시야, 하루, 하루나, 치즈코
  Support Party: 리코, 미유, 마나카, 유우미, 카요, 후타바, 후카
  {% endcomment %}
  
  {% assign characters = "나루미,준페이,J&C,슌·프론티어, 후카,쇼키·암야,YUI·스텔라,미유·여름,카타야마,렌,루우나,루페르,레오,류지,리코·매화,마사키,마유미,마코토,미나미,미나미·여름,모르가나,모토하,모토하·여름,몽타뉴,몽타뉴·백조,미오,사나다,슌,세이지,안,아야카,아케치,야오링,야오링·사자무,유스케, YUI,유카리,유키미,유키 마코토,쇼키,이치고,카스미,키라,키요시,토모코,토모코·여름,토시야,하루,하루나,치즈코,리코,미유,마나카,유우미,카요,후타바" | split: "," %}
  {% assign languages = "kr,en,jp" | split: "," %}
  
  {% for character in characters %}
    {% unless character == "원더" %}
      {% assign encoded_name = character | url_encode %}
      
      <!-- Character: {{ character }} -->
      <url>
        <loc>{{ site.url }}/character.html?name={{ encoded_name }}</loc>
        <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.7</priority>
        {% for lang in languages %}
          <xhtml:link rel="alternate" hreflang="{{ lang }}" href="{{ site.url }}/character.html?name={{ encoded_name }}&amp;lang={{ lang }}" />
        {% endfor %}
        <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/character.html?name={{ encoded_name }}" />
      </url>
      
      <!-- Character with explicit language parameters -->
      {% for lang in languages %}
        <url>
          <loc>{{ site.url }}/character.html?name={{ encoded_name }}&amp;lang={{ lang }}</loc>
          <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
          <changefreq>monthly</changefreq>
          <priority>0.6</priority>
          {% for alt_lang in languages %}
            <xhtml:link rel="alternate" hreflang="{{ alt_lang }}" href="{{ site.url }}/character.html?name={{ encoded_name }}&amp;lang={{ alt_lang }}" />
          {% endfor %}
          <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/character.html?name={{ encoded_name }}" />
        </url>
      {% endfor %}
    {% endunless %}
  {% endfor %}

  <!-- Persona Detail Pages from data/persona/*.js -->
  {% assign languages = "kr,en,jp" | split: "," %}
  {% assign persona_files = site.static_files | where_exp: "file", "file.path contains '/data/persona/'" %}
  {% for file in persona_files %}
    {% if file.extname == '.js' %}
      {% assign filename = file.basename | remove: '.js' %}
      {% unless filename == 'order' or filename == 'nonorder' or filename == 'persona-list' or filename == 'persona-search' or filename == 'persona-routing' or filename == 'skillfrom' or filename == 'getfrom' %}
        {% assign encoded_name = filename | url_encode %}
        <url>
          <loc>{{ site.url }}/persona/?name={{ encoded_name }}</loc>
          <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
          <changefreq>monthly</changefreq>
          <priority>0.7</priority>
          {% for lang in languages %}
            <xhtml:link rel="alternate" hreflang="{{ lang }}" href="{{ site.url }}/persona/?name={{ encoded_name }}&amp;lang={{ lang }}" />
          {% endfor %}
          <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/persona/?name={{ encoded_name }}" />
        </url>
        
        <!-- Persona with explicit language parameters -->
        {% for lang in languages %}
          <url>
            <loc>{{ site.url }}/persona/?name={{ encoded_name }}&amp;lang={{ lang }}</loc>
            <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
            <changefreq>monthly</changefreq>
            <priority>0.6</priority>
            {% for alt_lang in languages %}
              <xhtml:link rel="alternate" hreflang="{{ alt_lang }}" href="{{ site.url }}/persona/?name={{ encoded_name }}&amp;lang={{ alt_lang }}" />
            {% endfor %}
            <xhtml:link rel="alternate" hreflang="x-default" href="{{ site.url }}/persona/?name={{ encoded_name }}" />
          </url>
        {% endfor %}
      {% endunless %}
    {% endif %}
  {% endfor %}
</urlset>
